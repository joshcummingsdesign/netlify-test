{
  "name": "netlify-test",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "PORT=8080 react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "cypress": "cypress open",
    "cy:run": "cypress run",
    "cy:record": "cypress run --record",
    "start:ci": "http-server build -c-1 --silent",
    "e2e:run": "yarn build && start-server-and-test start:ci 8080 cy:run",
    "e2e:record": "yarn build && start-server-and-test start:ci 8080 cy:record",
    "predeploy": "yarn audit && yarn test && yarn add cypress@3.3.1 && yarn e2e:record"
  },
  "dependencies": {
    "react": "^16.8.6",
    "react-dom": "^16.8.6"
  },
  "devDependencies": {
    "cypress": "^3.3.1",
    "http-server": "^0.11.1",
    "react-scripts": "3.0.1",
    "start-server-and-test": "^1.9.1"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
